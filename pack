echo "checking if $1 exists"
if [ ! -f "$1" ]; then
    echo "finding match for $1"
    if [ $1 == "v" ]; then
        # for building v
        git clone https://github.com/vlang/v
    else
        echo "no match found for $1 aborting"
fi
# uninstaller
if [ $1 == "-d" ]; then
    if [ ! -f $2 ]; then
        rm -rf $2 || rm -f $2
    fi
fi
